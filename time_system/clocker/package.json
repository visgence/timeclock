  "name": "Timeclock",
  "private": "true",
  "scripts": {
    "lint": "npm run htmllint && npm run csslint && npm run eslint && npm run pythonlint",
    "pythonlint": "pep8 --exclude='*node_modules*' . --max-line-length=200",
    "htmllint": "./node_modules/.bin/htmlhint --config htmlhint.conf ./../templates/",
    "eslint": "./node_modules/.bin/eslint ./js",
    "csslint": "./node_modules/.bin/stylelint ./css/*.css",
    "buildProduction": "npm run clean && npm run webpackProduction",
    "build": "npm run clean && npm run watch",
    "clean": "rm -fr static/dist/",
    "watch": "webpack --watch --progress --config ./webpack.development.config.js",
    "webpackProduction": "webpack --progress --config ./webpack.production.config.js",
    "webpack": "webpack --progress --config ./webpack.development.config.js"
  },
  "version": "1.0.0"
